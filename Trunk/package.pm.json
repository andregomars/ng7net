{
  "name": "aspnetnetcore",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "aspnetcore",
    "entityframework core",
    "angular",
    "webpack",
    "typescript",
    "bootstrap4",
    "ng-bootstrap",
    "ngx-translate"
  ],
  "author": "Majid Mashhadi<mmashhadi@papermart.com>",
  "scripts": {
    "tslint": "tslint",
    "protractor": "protractor",
    "clean": "rimraf papermart.db bin ClientApp/dist wwwroot/dist",
    "dev:watch": "set ASPNETCORE_ENVIRONMENT=Development && dotnet watch run",
    "prod:watch": "set ASPNETCORE_ENVIRONMENT=Production && dotnet watch run",
    "build": "node_modules/.bin/ng build",
    "test": "node_modules/.bin/ng test",
    "lint": "npm run tslint \"ClientApp/**/*.ts\"",
    "postinstall": "npm run webdriver:update",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:debug": "protractor --elementExplorer",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
